{% extends 'base.html.twig' %}
{% block debut %}

	<section class="testimonial-section spad">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
					<div class="section-title">
						<span>Exercice</span>
						<h2>{{exercice.nomExercice}}</h2>
					</div>
				</div>
			</div>
			<div class="ts_slider owl-carousel">

				<div class="ts_item">
					<div class="row">
						<div class="col-lg-12 text-center">
							<div class="ti_pic">
								<img src={{ asset('uploads/exercices/' ~ exercice.imageExercice) }} width="300" height="300"/>

							</div>
							<div class="ti_text">
								<h5>{{exercice.instruction}}</h5>
								<h5>Duration :{{exercice.duration}}</h5>
								<h5>Repetition :{{exercice.repetation}}</h5>
								<div class="tt-rating">
									<i class="fa fa-star"></i>
									<i class="fa fa-star"></i>
									<i class="fa fa-star"></i>
									<i class="fa fa-star"></i>
									<i class="fa fa-star"></i>
								</div>
							</div>
						</div>

					</div>
				</div>
			</div>
		</div>
		<a href="https://www.youtube.com/watch?v=EzKkl64rRbM" class="play-btn video-popup">
			<i class="fa fa-caret-right"></i>
		</a>
		<div class="col-lg-12 text-center">
			<div
				class="bs-text">
				<!-- en principe listexercicef -->
				<a href="{{path('listSerieF') }}" class="primary-btn  btn-normal">Retour</a>
			</div>
		</div>
	</section>


	<!-- Testimonial Section End -->
{% endblock %}
